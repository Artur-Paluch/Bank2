<UserControl x:Class="Bank2.Components.RodzajLokatyItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Bank2.Components"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel Orientation="Horizontal">
        <TextBlock Text="{Binding Id_rodzaju_Lokaty}"/>
        <TextBlock Text="{Binding Nazwa }"/>
        <TextBlock Text="{Binding Oprocentowanie}"/>
        <TextBlock Text="{Binding Okres_w_mies}"/>
        <TextBlock Text="{Binding Prowizja }"/>
        <TextBlock Text="{Binding UtworzylImie }"/>
        <TextBlock Text="{Binding UtworzylNazwisko}"/>

    </StackPanel>
</UserControl>
